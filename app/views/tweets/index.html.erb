<div id="wrapper">

<p id="notice"><%= notice %></p>


<div class="coment" align="center">
<table>
  <thead>
    <tr>
      <th class="tweetList">Tweet一覧</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tweets.each do |tweet| %>
      <tr>
        <td class="btn btn_edit"><%= link_to '編集', edit_tweet_path(tweet),class: 'btn btn-default' %></td>
        <td><%= tweet.content %></td>
        <td><%= link_to '消去', tweet_path(tweet), method: :delete, data: { confirm: 'Are you sure?' },class: 'btn btn-default' %></td>
      </tr>
    <% end %>
  </tbody>
</table>

</div>

<br>

<div class="new_comment">
<%= link_to 'New Tweet', new_tweet_path %>
</div>
</div>
</div>