<p id="notice"><%= notice %></p>


<table>
  <thead>
    <tr>
      <th>DDDDDDD</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tweets.each do |tweet| %>
      <tr>
        <td><%= tweet.content %></td>
        <td><%= link_to '編集', edit_tweet_path(tweet),class: 'btn btn-default' %></td>
        <td><%= link_to '消去', tweet_path(tweet), method: :delete, data: { confirm: 'Are you sure?' },class: 'btn btn-default' %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tweet', new_tweet_path,class: 'btn btn-default' %>
 
</div>